<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="355.0" prefWidth="568.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.TestController">
   <children>
      <HBox alignment="CENTER" prefHeight="48.0" prefWidth="600.0">
         <children>
            <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="48.0" prefWidth="514.0" text="Check Strings (input)" textAlignment="CENTER">
               <font>
                  <Font name="Arial Black" size="20.0" />
               </font>
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </Label>
            <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="48.0" prefWidth="345.0" text="Status" textAlignment="CENTER">
               <font>
                  <Font name="Arial Black" size="20.0" />
               </font>
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </Label>
         </children>
      </HBox>
      <HBox>
         <children>
            <TextArea fx:id="inputs" prefHeight="317.0" prefWidth="600.0" promptText="Enter Inputs">
               <font>
                  <Font name="SansSerif Bold" size="20.0" />
               </font>
            </TextArea>
            <TextArea fx:id="status" editable="false" prefHeight="317.0" prefWidth="427.0">
               <font>
                  <Font name="SansSerif Bold" size="20.0" />
               </font>
            </TextArea>
         </children>
      </HBox>
      <HBox alignment="CENTER_LEFT">
         <children>
            <Button mnemonicParsing="false" onAction="#OnClickCheckBtn" prefHeight="34.0" prefWidth="151.0" text="Check">
               <font>
                  <Font name="Arial Black" size="20.0" />
               </font>
               <HBox.margin>
                  <Insets left="120.0" />
               </HBox.margin>
            </Button>
         </children>
         <VBox.margin>
            <Insets bottom="20.0" top="20.0" />
         </VBox.margin>
      </HBox>
   </children>
   <padding>
      <Insets top="10.0" />
   </padding>
</VBox>
